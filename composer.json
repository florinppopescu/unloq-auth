{
    "name": "unloq/unloq-php-sdk",
    "description": "UNLOQ.io PHP SDK provides methods for all of UNLOQ.io functionalities",
    "keywords": ["UNLOQ", "Authentication", "UNLOQ Two Factor Authentication", "Two Factor Authentication", "2fa", "php 2fa"],
    "license": "MIT",
    "authors": [
        {
            "name": "Florin Popescu",
            "email": "florin@unloq.io"
        }
    ],
    "type": "project",
    "require": {
		"php": ">=5.6"
    },
	"require-dev": {
		"phpunit/phpunit": "6.2.*",
		"guzzlehttp/guzzle": "~6.0"
    },
	"autoload": {
        "psr-4": {
            "Unloq": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Unloq\\Tests\\": "tests/"
        }
    }
}
